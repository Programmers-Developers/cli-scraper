      "Test\\": "tests",
      "Test\\Controller\\": "tests/Controller"
    }
  },
  "authors": [
    {
      "name": "Brigido Alejo",
      "email": "brigsalejo@gmail.com"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "5.5.*",
    "behat/behat": "~2.5"
  },
  "scripts": {
    "test" : "phpunit"
  },
  "minimum-stability": "stable"
}
