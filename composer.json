    "doctrine/orm": "v2.5.4",
    "symfony/yaml": "2.*",
    "guzzlehttp/guzzle": "~6.0",
    "guiguiboy/php-cli-progress-bar": "@dev",
    "fabpot/goutte": "^3.1"
  },
  "autoload": {
    "psr-4": {
